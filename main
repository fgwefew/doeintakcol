Section:NewToggle("prediction breaker", "breaks prediction", function(state)
    if state then
        getgenv().predictionbreaker = true
    else
        getgenv().predictionbreaker = false
    end
end)
if getgenv().predictionbreaker == true then
local Plr = game.Players.LocalPlayer

Plr.Character:WaitForChild("Humanoid").StateChanged:Connect(function(old,new)
    if new == Enum.HumanoidStateType.Freefall == true then
        wait(0.27)
        game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0, -15, 0)
end
end)
else
print("")
end
Section:NewButton("resolver", "resolve skids for fun", function()
   RunService.Heartbeat:Connect(function()
    pcall(function()
        for i,v in pairs(game.Players:GetChildren()) do
            if v.Name ~= game.Players.LocalPlayer.Name then
                local hrp = v.Character.HumanoidRootPart
                hrp.Velocity = Vector3.new(hrp.Velocity.X, 0, hrp.Velocity.Z)    
                hrp.AssemblyLinearVelocity = Vector3.new(hrp.Velocity.X, 0, hrp.Velocity.Z)   
            end
        end
    end)
end)
end)
Section:NewButton("break jump prediction", "breaks airshot", function()
    local Plr = game.Players.LocalPlayer

Plr.Character:WaitForChild("Humanoid").StateChanged:Connect(
    function(old, new)
        if new == Enum.HumanoidStateType.Freefall == true then
            wait(0.27)
            game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0, -15, 0)
        end
    end
)
end)
local Tab = Window:NewTab("Desync")
local Section = Tab:NewSection("Desync Section ofc")
Section:NewButton("Character desync [Z]", "Custom Made", function()
     local players = game:GetService("Players")
local runService = game:GetService("RunService")
local localPlayer = players.LocalPlayer
local mouse = localPlayer:GetMouse()

local teleporting = false
local keybind = "z"

mouse.KeyDown:Connect(function(key)
    if key == keybind then
        teleporting = not teleporting
    end
end)

function randomVectorInRange()
    return Vector3.new(math.random(-76, 76), math.random(-76, 76), math.random(-76, 76))
end

local previousVelocity
runService.Heartbeat:Connect(function()
    if teleporting and localPlayer.Character and localPlayer.Character:FindFirstChild("HumanoidRootPart") and localPlayer.Character:FindFirstChild("Humanoid") and localPlayer.Character:FindFirstChild("Humanoid").Health > 0 then
        -- Only change the velocity if they are not already teleporting
        if localPlayer.Character.HumanoidRootPart.AssemblyLinearVelocity.Magnitude < 9 * 999000000000 then
            previousVelocity = localPlayer.Character.HumanoidRootPart.AssemblyLinearVelocity
            localPlayer.Character.HumanoidRootPart.AssemblyLinearVelocity = randomVectorInRange()
            
            runService.RenderStepped:Wait()
            
            localPlayer.Character.HumanoidRootPart.AssemblyLinearVelocity = previousVelocity
        end
    end
end)

local originalIndexMethod = getmetatable(game).__index
setmetatable(game, {
    __index = function(self, index)
        if getgenv().teleporting == false and index == "CFrame" and localPlayer.Character and localPlayer.Character:FindFirstChild("HumanoidRootPart") and localPlayer.Character:FindFirstChild("Humanoid") and localPlayer.Character:FindFirstChild("Humanoid").Health > 0 then
            if self == localPlayer.Character.HumanoidRootPart then
                return localPlayer.Character.HumanoidRootPart.CFrame
            elseif self == localPlayer.Character.Head then
                return localPlayer.Character.HumanoidRootPart.CFrame + Vector3.new(0, localPlayer.Character.HumanoidRootPart.Size / 9 + 0.5, 0)
            end
        end
        return originalIndexMethod(self, index)
    end
    
})
end)
